{% extends '@banku/layout/base.twig' %}

{% block title %}Accounts{% endblock %}

{% block content %}
    <h3>Accounts</h3>
    <hr>
    {% for account in accounts %}
        <div>
            No: {{ account.no }}
            Type: {{ account.type }}
            Status: {{ account.status }}
        </div>
    {% endfor %}
{% endblock %}