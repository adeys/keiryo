{
    "name": "hackr/keiryo",
    "description": "Lightweight and simple components",
    "type": "library",
    "license": "MIT",
    "require": {
		"php": ">=7.2.0",
		"ext-json": "*",
		"psr/event-dispatcher": "^1.0.0",
		"rakit/validation": "^1.0.0",
		"symfony/config": "^4.2",
		"symfony/console": "^4.2",
		"symfony/finder": "^4.2",
		"symfony/http-foundation": "^4.2",
		"symfony/routing": "^4.2",
		"symfony/yaml": "^4.2",
		"twig/twig": "2.*"

    },
    "require-dev": {
      "league/container": "^3",
      "symfony/http-foundation": "^4.0",
		"robmorgan/phinx": "dev-master",
      "tracy/tracy": "^2.5",
      "fzaninotto/faker": "^1.7",
      "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Keiryo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keiryo\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "K. Raphael",
            "email": "raphalogou@gmail.com"
        }
    ]
}
