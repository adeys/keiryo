{
    "name": "hackr/simplex",
    "description": "Simple practice",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "ext-pdo": "*",
        "league/container": "^3",
        "symfony/http-foundation": "^4.0",
        "symfony/routing": "^4.0",
        "symfony/config": "^4.0",
        "symfony/yaml": "^4.0",
        "twig/twig": "^2",
        "finesse/query-scribe": "*"
    },
    "require-dev": {
        "robmorgan/phinx": "^0.9",
        "fzaninotto/faker": "^1.7",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
