{
    "name": "hackr/keiryo",
    "description": "Lightweight and simple components",
    "type": "library",
    "license": "MIT",
    "require": {
      "php": ">=7.2.0",
      "ext-json": "*"
    },
    "require-dev": {
      "league/container": "^3",
      "symfony/http-foundation": "^4.0",
      "robmorgan/phinx": "^0.9",
      "tracy/tracy": "^2.5",
      "fzaninotto/faker": "^1.7",
      "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Keiryo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keiryo\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "K. Raphael",
            "email": "raphalogou@gmail.com"
        }
    ]
}
