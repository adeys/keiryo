{
    "name": "hackr/keiryo",
    "description": "Lightweight and simple components",
    "type": "library",
    "license": "MIT",
    "require": {
		"php": ">=7.2.0",
		"ext-json": "*",
		"psr/event-dispatcher": "^1.0.0",
		"rakit/validation": "^1.0.0",
		"symfony/config": "4.4.*",
		"symfony/console": "4.4.*",
		"symfony/finder": "4.4.*",
		"symfony/http-foundation": "4.4.*",
		"symfony/routing": "4.4.*",
		"symfony/yaml": "4.4.*",
		"twig/twig": "3.*"

    },
    "require-dev": {
      "league/container": "3.*",
      "symfony/http-foundation": "4.4.*",
	  "robmorgan/phinx": "0.*",
      "tracy/tracy": "2.*",
      "fzaninotto/faker": "1.*",
      "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Keiryo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keiryo\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "K. Raphael",
            "email": "raphalogou@gmail.com"
        }
    ]
}
